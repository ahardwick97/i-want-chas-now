{% extends 'base.html.twig' %}
{% block title %}{{ page.metaTitle|raw }}{% endblock %}
{% block body %}
    <div class="main">
        <br>
        <div class="container-fluid">
            <div class="row">
                <div class="col-sm-6 ">
                    <div class="title">
                        {{ page.title|raw }}
                    </div>
                    <br>
                    <div class="content">

                        {{ page.content|raw }}

                        {% include "forms/contact-form.html.twig" %}
                        <br>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="image-border">
                        <br>
                        <br>
                        <br>
                        <br>
                        <img src="{{ asset('images/mapImages/' ~ page.image) }}" style="max-width: 100%;">
                    </div>
                    <br>
                    <br>
                </div>

            </div>
        </div>
    </div>
    <br>

{% endblock %}

